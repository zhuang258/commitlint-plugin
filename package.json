{
  "name": "commitlint-plugin-jira-plugin",
  "version": "1.0.0",
  "description": "My custom commitlint plugin",
  "main": "index.js",
  "license": "ISC",
  "commitlint": {
    "plugins": [
      "./index"
    ],
    "rules": {
      "jira-task-id": [
        2,
        "always"
      ]
    }
  },
  "dependencies": {
    "@commitlint/cli": "^17.6.5"
  }
}